<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <!--[if lt IE 9]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <meta http-equiv="Content-Language" content="en"/>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="/images/favicon.256x256.png">

    <!-- Page Title -->
    <title><%= title %></title>

    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300,400italic,600,700,900,200'
          rel='stylesheet' type='text/css'>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">

    <!-- Custom css file -->
    <link rel="stylesheet" href="/css/style.css">

    <!-- Plugins -->
    <link rel="stylesheet" href="/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/animate.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--META-->
    <meta name="robots" content="noindex,nofollow"/>

</head>
<body>

<main class="container fullscreen">

    <!-- Mobile Menu -->
    <%- include ./common/mobileMenu.ejs %>
    <!-- /Mobile Menu -->

    <!-- Header -->
    <header class="transition header fixed">
        <!-- Menu -->
        <%- include ./common/menu.ejs %>
        <!-- /Menu -->
    </header>
    <!-- /Header -->

    <!-- Content -->
    <div class="container center error404">
        <div class="dt fullscreen">
            <div class="dtc">
                <i class="fa fa-compass"></i>

                <h1>Error 404 - Page Not Found</h1>
                <p>The page you were looking for wasn't found</p>
                <a href="/" class="btn"><span>Back home</span></a>
            </div>
        </div>
    </div>
    <!-- /Content -->

    <!--Border-->
    <div class="border">
        <div class="frames">
            <div></div>
            <div></div>
            <div></div>
            <div></div>
        </div>
        <div class="corners">
            <div></div>
            <div></div>
        </div>
    </div>
    <!--/Border-->

    <!-- Footer -->
    <footer id="footer" class="fixed white">
        <p>&copy; Bogdan Alexandrov 2017. All rights reserved.</p>
    </footer>
    <!-- /Footer -->
</main>

<!-- JS -->
<%- include ./common/javascript.ejs %>
<!-- Plugins -->
<script type="text/javascript" src="/js/website-smooth-scroll.min.js"></script>
<!-- Main js -->
<script type="text/javascript" src="/js/main.js"></script>

</body>
</html>